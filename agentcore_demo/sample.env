# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=<your-aws-account-id>

# Bedrock AgentCore Configuration
AGENT_RUNTIME_ARN=arn:aws:bedrock-agentcore:<region>:<account-id>:runtime/<agent-id>
EXECUTION_ROLE_ARN=arn:aws:iam::<account-id>:role/service-role/<execution-role-name>
ECR_REPOSITORY=<account-id>.dkr.ecr.<region>.amazonaws.com/<repository-name>

# CodeBuild Configuration
CODEBUILD_PROJECT_NAME=<codebuild-project-name>
CODEBUILD_EXECUTION_ROLE=arn:aws:iam::<account-id>:role/<codebuild-role-name>
SOURCE_BUCKET=<source-bucket-name>

# Agent Configuration
AGENT_ID=<your-agent-id>
AGENT_SESSION_ID=<your-session-id>

# Third-party API Keys (optional)
GOOGLE_CLIENT_ID=<your-google-client-id>
GOOGLE_CLIENT_SECRET=<your-google-client-secret>
PERPLEXITY_API_KEY=<your-perplexity-api-key>
