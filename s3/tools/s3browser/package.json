{
  "name": "s3browser",
  "version": "1.0.0",
  "description": "Cross-platform S3 Browser desktop application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "test": "jest"
  },
  "build": {
    "appId": "com.s3browser.app",
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "s3icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "s3icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "s3icon.png"
    }
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/s3-request-presigner": "^3.732.0",
    "electron-store": "^8.1.0",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.0.0",
    "jest": "^29.0.0",
    "png2icons": "^2.0.1"
  }
}
